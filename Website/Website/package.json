{
  "name": "datax-config",
  "version": "1.0.0",
  "description": "A simplified template based on express+react+webpack.",
  "main": "server.js",
  "engines": {
    "node": "10.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/data-accelerator"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js --dist=prod",
    "devstart": "node server.js",
    "build": "rimraf dist\\prod && webpack --mode production --config webpack.config.js --progress",
    "buildwatch": "webpack --mode production --config webpack.config.js  --progress  --watch",
    "dev": "rimraf dist\\dev && webpack --mode development --config webpack.config.js --env.dist \"dev\"  --progress",
    "devwatch": "webpack --mode development --config webpack.config.js --env.dist \"dev\" --progress --watch",
    "deploy": "call ./deploy/deploy.cmd"
  },
