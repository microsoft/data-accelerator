{
  "name": "datax-config",
  "version": "1.0.0",
  "description": "A simplified template based on express+react+webpack.",
  "main": "server.js",
  "engines": {
    "node": "10.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/data-accelerator"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js --dist=prod",
    "devstart": "node server.js",
    "build": "rimraf dist\\prod && webpack --mode production --config webpack.config.js --env.dist \"prod\" --bail --progress --display-modules --display-error-details --colors",
    "buildwatch": "webpack --mode production --config webpack.config.js --env.dist \"prod\" --progress --display-modules --display-error-details --colors --watch",
    "dev": "rimraf dist\\dev && webpack --mode development --config webpack.config.js --env.dist \"dev\" --bail --progress --display-modules --display-error-details --colors --display-chunks --display-reasons",
    "devwatch": "webpack --mode development --config webpack.config.js --env.dist \"dev\" --progress --display-modules --display-error-details --colors --display-chunks --display-reasons --watch",
    "deploy": "call ./deploy/deploy.cmd"
  },
  "author": "Microsoft",
  "license": "MIT",
